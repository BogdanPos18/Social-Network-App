<AnchorPane prefHeight="328.0" prefWidth="506.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6.Controller.LoginController">
   <children>
      <TextField fx:id="loginName" layoutX="178.0" layoutY="110.0" />
      <Label layoutX="178.0" layoutY="94.0" text="Username">
         <font>
            <Font name="Wingdings 2" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DISAPPEAR" />
         </cursor>
         <effect>
            <Blend />
         </effect>
      </Label>
      <Button fx:id="loginButton" layoutX="199.0" layoutY="151.0" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="26.0" prefWidth="106.0" text="Login" />
   </children>
   <effect>
      <DropShadow />
   </effect>
</AnchorPane>
