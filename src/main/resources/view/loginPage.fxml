<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="386.0" prefWidth="506.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6.Controller.LoginController">
   <children>
      <TextField fx:id="loginName" layoutX="178.0" layoutY="81.0" />
      <Label layoutX="178.0" layoutY="65.0" text="Username">
         <font>
            <Font name="Wingdings 2" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DISAPPEAR" />
         </cursor>
         <effect>
            <Blend />
         </effect>
      </Label>
      <Button fx:id="loginButton" layoutX="199.0" layoutY="220.0" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="34.0" prefWidth="106.0" text="Log in" />
      <Label layoutX="178.0" layoutY="147.0" text="Password">
         <font>
            <Font name="Wingdings 2" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DISAPPEAR" />
         </cursor>
         <effect>
            <Blend />
         </effect>
      </Label>
      <Button fx:id="firstSignupButton" layoutX="200.0" layoutY="307.0" mnemonicParsing="false" onAction="#signupButtonClicked" prefHeight="34.0" prefWidth="106.0" text="Sign up" />
      <PasswordField fx:id="password" layoutX="179.0" layoutY="163.0" />
      <Label layoutX="170.0" layoutY="289.0" text="Don't have an account? Sign up" />
   </children>
   <effect>
      <DropShadow />
   </effect>
</AnchorPane>
