<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6.Controller.SignupController">
   <children>
      <TextField fx:id="signName" layoutX="226.0" layoutY="120.0" />
      <Label layoutX="226.0" layoutY="104.0" text="Give a username">
         <font>
            <Font name="Wingdings 2" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DISAPPEAR" />
         </cursor>
         <effect>
            <Blend />
         </effect>
      </Label>
      <Label layoutX="226.0" layoutY="192.0" text="Give a password">
         <font>
            <Font name="Wingdings 2" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DISAPPEAR" />
         </cursor>
         <effect>
            <Blend />
         </effect>
      </Label>
      <Button fx:id="signupButton" layoutX="269.0" layoutY="294.0" mnemonicParsing="false" onAction="#signupButtonPressed" text="Sign up" />
      <PasswordField fx:id="signPassword" layoutX="226.0" layoutY="208.0" />
   </children>
</AnchorPane>
